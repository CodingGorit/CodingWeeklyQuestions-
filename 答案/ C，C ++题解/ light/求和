#include <stdio.h>
#include <stdlib.h>

int main()
{
    int X,N,sum = 0,t = 0,i;
    scanf("%d %d",&X,&N);
    if(X >= 1 && X <= 9 && N >= 0 && X <= 9)
    {
        for(i = 1;i <= N;i++)
        {
            t = t*10 + X;
            sum += t;
        }
        printf("%d",sum);
    }
    else
        printf("ERR!\n");
    return 0;
}
